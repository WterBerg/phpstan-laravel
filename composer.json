{
    "name": "wterberg/phpstan-laravel",
    "type": "phpstan-extension",
    "description": "Contains several custom rules to analyse Laravel projects with PHPStan.",
    "keywords": ["PHPStan", "Laravel"],
    "homepage": "https://github.com/WterBerg/phpstan-laravel",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/WterBerg/phpstan-laravel/issues",
        "source": "https://github.com/WterBerg/phpstan-laravel",
        "docs": "https://github.com/WterBerg/phpstan-laravel/blob/main/README.md"
    },
    "authors": [
        {
            "name": "Willem ter Berg",
            "email": "wrpterberg@gmail.com",
            "homepage": "https://wterberg.nl/"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "WterBerg\\PHPStanLaravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0 || ^9.0",
        "nikic/php-parser": "^4.13",
        "phpstan/phpstan": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^8.5 || ^9.3"
    }
}
